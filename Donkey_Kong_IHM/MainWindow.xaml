<Window x:Class="Donkey_Kong_IHM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Donkey_Kong_IHM"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Grid Background="black">
        <Canvas x:Name="canvas" Background="Black" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        <Button Content="Play" TextBlock.Foreground="White" TextBlock.FontSize="40" Margin="300,292,300,210" Background="DeepSkyBlue" Click="Jouer"/>
        <Image Source="Ressources/Image/acceuil/ddddddd-Photoroom.png" Margin="162,421,162,19"/>
        <Image x:Name="titre" Source="Ressources/Image/acceuil/NES - Donkey Kong - Title Screen-Photoroom.png" Margin="188,9,200,341" RenderTransformOrigin="1.209,0.863"/>
    </Grid>
</Window>
